{
  "name": "Lemonade",
  "type": "dark",
  "colors": {
    "editor.background": "#2A2D34",
    "editor.foreground": "#E8E8E8",
    "activityBar.background": "#1E2024",
    "activityBar.foreground": "#E8E8E8",
    "activityBarBadge.background": "#FF6B6B",
    "activityBarBadge.foreground": "#FFFFFF",
    "sideBar.background": "#1E2024",
    "sideBar.foreground": "#E8E8E8",
    "sideBarTitle.foreground": "#E8E8E8",
    "statusBar.background": "#1E2024",
    "statusBar.foreground": "#E8E8E8",
    "titleBar.activeBackground": "#1E2024",
    "titleBar.activeForeground": "#E8E8E8",
    "tab.activeBackground": "#2A2D34",
    "tab.inactiveBackground": "#1E2024",
    "tab.activeForeground": "#E8E8E8",
    "tab.inactiveForeground": "#8B8B8B",
    "editorGroupHeader.tabsBackground": "#1E2024",
    "editorGroup.border": "#3A3D44",
    "panel.background": "#1E2024",
    "panel.border": "#3A3D44",
    "terminal.background": "#1E2024",
    "terminal.foreground": "#E8E8E8",
    "terminalCursor.foreground": "#FF6B6B",
    "terminalCursor.background": "#E8E8E8",
    "terminal.selectionBackground": "#3A3D44",
    "terminal.selectionForeground": "#E8E8E8",
    "terminal.ansiBlack": "#1E2024",
    "terminal.ansiBlue": "#4ECDC4",
    "terminal.ansiCyan": "#4ECDC4",
    "terminal.ansiGreen": "#95E1D3",
    "terminal.ansiMagenta": "#FF6B6B",
    "terminal.ansiRed": "#FF6B6B",
    "terminal.ansiWhite": "#E8E8E8",
    "terminal.ansiYellow": "#FFD93D",
    "terminal.ansiBrightBlack": "#3A3D44",
    "terminal.ansiBrightBlue": "#4ECDC4",
    "terminal.ansiBrightCyan": "#4ECDC4",
    "terminal.ansiBrightGreen": "#95E1D3",
    "terminal.ansiBrightMagenta": "#FF6B6B",
    "terminal.ansiBrightRed": "#FF6B6B",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#FFD93D"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A6A6A"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#E8E8E8"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#95E1D3"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFD93D"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute",
        "meta.attribute.html",
        "meta.attribute.jsx",
        "meta.attribute.tsx",
        "meta.attribute.vue"
      ],
      "settings": {
        "foreground": "#95E1D3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML Attribute Values",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.vue",
        "string.quoted.single.vue"
      ],
      "settings": {
        "foreground": "#FFD93D"
      }
    },
    {
      "name": "JSX/TSX Expressions",
      "scope": [
        "meta.embedded.expression.jsx",
        "meta.embedded.expression.tsx",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "Vue Template Tags",
      "scope": [
        "entity.name.tag.template.vue",
        "entity.name.tag.style.vue",
        "entity.name.tag.script.vue"
      ],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "Vue Directives",
      "scope": [
        "entity.other.attribute-name.directive.vue",
        "entity.other.attribute-name.directive.argument.vue",
        "entity.other.attribute-name.directive.modifier.vue"
      ],
      "settings": {
        "foreground": "#95E1D3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#95E1D3"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "Markdown Headers",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#FFD93D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#95E1D3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML Tag Delimiters",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.begin.jsx",
        "punctuation.definition.tag.end.jsx",
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.tsx",
        "punctuation.definition.tag.begin.vue",
        "punctuation.definition.tag.end.vue",
        "punctuation.definition.tag.xml",
        "punctuation.definition.tag.end.xml"
      ],
      "settings": {
        "foreground": "#AEEBFF"
      }
    },
    {
      "name": "HTML Self-closing Tag Delimiters",
      "scope": [
        "punctuation.definition.tag.self-closing.html",
        "punctuation.definition.tag.self-closing.jsx",
        "punctuation.definition.tag.self-closing.tsx",
        "punctuation.definition.tag.self-closing.vue",
        "punctuation.definition.tag.self-closing.xml"
      ],
      "settings": {
        "foreground": "#AEEBFF"
      }
    },
    {
      "name": "HTML Tag Brackets",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx",
        "punctuation.definition.tag.vue",
        "punctuation.definition.tag.xml"
      ],
      "settings": {
        "foreground": "#AEEBFF"
      }
    },
    {
      "name": "String Quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json",
        "punctuation.definition.string.begin.js",
        "punctuation.definition.string.end.js",
        "punctuation.definition.string.begin.ts",
        "punctuation.definition.string.end.ts",
        "punctuation.definition.string.begin.jsx",
        "punctuation.definition.string.end.jsx",
        "punctuation.definition.string.begin.tsx",
        "punctuation.definition.string.end.tsx",
        "punctuation.definition.string.begin.python",
        "punctuation.definition.string.end.python"
      ],
      "settings": {
        "foreground": "#AEEBFF"
      }
    },
    {
      "name": "Semicolon",
      "scope": [
        "punctuation.terminator.statement",
        "punctuation.terminator.statement.js",
        "punctuation.terminator.statement.ts",
        "punctuation.terminator.statement.jsx",
        "punctuation.terminator.statement.tsx",
        "punctuation.terminator.statement.css",
        "punctuation.terminator.statement.scss",
        "punctuation.terminator.statement.less"
      ],
      "settings": {
        "foreground": "#AEEBFF"
      }
    }
  ]
} 